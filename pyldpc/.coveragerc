# Configuration for coverage.py

[run]
branch = True
source = pyldpc
include = */pyldpc/*
omit = */setup.py
       */pyldpc/examples_utils/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
